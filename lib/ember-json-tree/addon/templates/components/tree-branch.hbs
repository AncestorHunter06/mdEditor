{{tree-leaf model=model
  inTree=true
  select=select
  selected=selected
  hover=hover
  nodeDepth=nodeDepth
  nodePath=nodePath
}}

{{#if model.isExpanded}}
  {{tree-view model=model.children
    select=select
    selected=selected
    hover=hover
    parentDepth=nodeDepth
    parentPath=nodePath
  }}
{{/if}}
