{{#object/md-object-table
  items=model
  header=label
  buttonText="Add Resource"
  attributes=attributes as |editing|
}}
    Editing: {{editing.name}}

    {{input/md-input value=editing.name label="Name" placeholder="Online Resource Name"}}

    {{input/md-input type="url" required=true value=editing.uri label="URI" placeholder="Online Resource URI"}}

    {{#file-picker class="md-file-picker" fileLoaded="" preview=true accept=".jpeg,.jpg,.png,.gif,.svg" readAs="readAsUri"}}
      <button type="button" class="btn btn-lg btn-info btn-block">{{fa-icon "bullseye"}} Click or Drop a File Here to Import Data</button>
    {{/file-picker}}

    {{input/md-input value=editing.protocol
    label="Protocol" placeholder="Protocol for accessing the Online Resource"}}

    {{input/md-textarea maxlength=500 value=editing.description label="Description"
    placeholder="Description of the Online Resource: Less than 500 characters"}}

    {{input/md-codelist value=editing.function mdCodeName="onlineFunction"
    label="Function" placeholder="Select function of the Online Resource"
    tooltip=true allowClear=true width="70%"}}

{{/object/md-object-table}}
{{yield}}
