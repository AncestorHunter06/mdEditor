  {{!-- {{#layout/md-card
  title=(concat "Distribution Section #" index)
  collapsible=true
  collapsed=false
  profilePath=profilePath
  data-spy=(concat "Distribution #" index)
  shadow=true
  spotlightEnabled=false
  }} --}}
  {{#layout/md-object-container
    title="Distribution"
    index=index
    profilePath=profilePath
    deleteItem=(action "deleteDistribution")
  }}
    {{!-- <button type="button" class="btn btn-md btn-danger btn-block md-btn-responsive card-buttons" {{action "deleteDistribution" index}}>
          {{fa-icon "times"}} Delete Section</button> --}}
    <div class="row">
      <div class="col-sm-6">
        {{input/md-textarea
          value=model.description
          rows=2
          maxHeight=300
          placeholder="Describe the distribution of the resource"
          label="Description"
          profilePath=(concat profilePath ".description")
        }}
      </div>
      <div class="col-sm-6">
        {{input/md-textarea
          value=model.liablityStatement
          rows=2
          maxHeight=300
          placeholder="Statement of the liability assumed by the provider of the resource"
          label="Liablity Statement"
          profilePath=(concat profilePath ".liablityStatement")
        }}
      </div>
    </div>
    {{!-- <hr>
      <h4 class="text-info">Distributors</h4>
        {{#if model.distributor.length}}
      <div class="row">
        <div class="col-sm-9">
          {{#each model.distributor as |dist|}}
          <div>
            {{#with model.contact as |party|}}
              {{html-safe (if party.role party.role "<em>No Roles assigned.</em>")}}
              (
              {{#each party.party as |contact index|}}
                  {{if index ", "}} {{control/md-contact-link contactId=contact.contactId}}
              {{else}}
                  <em class="text-muted">No Contacts assigned.</em>
              {{/each}}
              )
              {{else}}
              <em class="text-muted">No responsibility assigned.</em>
            {{/with}}
          </div>
          {{/each}}
        </div>
        <div class="col-sm-3">
          <button type="button" class="btn btn-md btn-success btn-block md-btn-responsive" {{action "editDistribution" index}}>
            {{fa-icon "pencil"}} Edit Distributors</button>
        </div>
      </div> --}}
      {{object/md-objectroute-table
        attributes="Contacts"
        items=model.distributor
        header="Distributors"
        shadow=false
        buttonText="Add Distributor"
        ellipsis=true
        previewTemplateTable="object/md-distributor/preview"
        editItem=editDistributor
        profilePath=(concat profilePath ".distributor")
        verticalButtons=false
        hideIndex=false
        condensed=false
        editOnAdd=true
        scrollToId=(concat "md-distributor-" index)
        data-spy=false
        routeParams=index
      }}
      {{!-- {{else}}
        <div class="alert alert-info">
          No Distributors to display.
        </div>
      {{/if}} --}}



  {{/layout/md-object-container}}
